{
  "name": "aias-platform",
  "version": "1.0.0",
  "description": "Enterprise-grade AI consultancy platform showcasing custom AI agents, workflow automation, and intelligent business solutions",
  "private": true,
  "license": "MIT",
  "author": {
    "name": "AIAS Platform Team",
    "email": "team@aias-platform.com",
    "url": "https://aias-platform.com"
  },
  "homepage": "https://aias-platform.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/aias-platform.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/aias-platform/issues",
    "email": "support@aias-platform.com"
  },
  "keywords": [
    "ai",
    "artificial-intelligence",
    "automation",
    "workflow",
    "saas",
    "multi-tenant",
    "enterprise",
    "typescript",
    "react",
    "nextjs",
    "supabase"
  ],
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toast": "^1.1.5",
    "@stripe/stripe-js": "^8.3.0",
    "@supabase/supabase-js": "^2.38.4",
    "@tailwindcss/typography": "^0.5.10",
    "canvas-confetti": "^1.9.4",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "framer-motion": "^11.0.0",
    "lucide-react": "^0.400.0",
    "next": "^14.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "resend": "^2.1.0",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:analyze": "ANALYZE=true vite build",
    "preview": "next start",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:cov": "vitest --coverage --reporter=html --reporter=text",
    "db:push": "cd apps/web && prisma db push",
    "db:migrate": "cd apps/web && prisma migrate deploy",
    "db:generate": "cd apps/web && prisma generate",
    "db:seed": "cd apps/web && tsx prisma/seed.ts",
    "db:studio": "cd apps/web && prisma studio",
    "queue:worker": "tsx src/workers/index.ts",
    "doctor": "tsx scripts/reality-check.ts",
    "smoke": "tsx scripts/smoke.ts",
    "ops": "tsx ops/cli.ts",
    "analyze": "npm run build:analyze",
    "a11y": "pa11y-ci",
    "a11y:playwright": "playwright test --grep @accessibility",
    "lighthouse": "lighthouse-ci autorun",
    "start-ci": "PORT=3000 next start -p 3000",
    "lhci": "lhci autorun --config=./lighthouserc.json || true",
    "vercel:login": "vercel login || true",
    "vercel:pull": "vercel pull --yes --environment=preview --token $VERCEL_TOKEN",
    "vercel:build": "vercel build --token $VERCEL_TOKEN",
    "vercel:deploy:preview": "vercel deploy --prebuilt --token $VERCEL_TOKEN",
    "vercel:deploy:prod": "vercel deploy --prebuilt --prod --token $VERCEL_TOKEN",
    "supa:login": "supabase login --token $SUPABASE_ACCESS_TOKEN",
    "supa:link": "supabase link --project-ref $SUPABASE_PROJECT_REF",
    "supa:migrate:plan": "supabase db changes commit --use-migra",
    "supa:migrate:apply": "supabase db push --local false",
    "supa:status": "supabase db remote commit --dry-run || true",
    "audit:deps": "npm audit --audit-level moderate",
    "audit:licenses": "license-checker --onlyAllow 'MIT;Apache-2.0;BSD-2-Clause;BSD-3-Clause;ISC'",
    "audit:security": "npm audit --audit-level high",
    "security:check": "tsx scripts/security-self-check.ts",
    "perf:budgets": "lighthouse-ci autorun",
    "perf:analyze": "npm run build:analyze && open dist/stats.html",
    "clean": "rm -rf node_modules dist .next",
    "install:all": "pnpm install",
    "docker:build": "docker build -t aias-consultancy .",
    "docker:run": "docker run -p 3000:3000 aias-consultancy",
    "docker:compose:up": "docker-compose up -d",
    "docker:compose:down": "docker-compose down",
    "deploy:staging": "npm run build && npm run docker:build && npm run docker:run",
    "deploy:production": "npm run build && npm run docker:build && docker-compose -f docker-compose.prod.yml up -d",
    "flags:init": "tsx scripts/feature-flags.ts init",
    "flags:list": "tsx scripts/feature-flags.ts list",
    "flags:toggle": "tsx scripts/feature-flags.ts toggle",
    "flags:export": "tsx scripts/feature-flags.ts export",
    "migrate:emc": "tsx scripts/migrate-emc.ts run",
    "migrate:status": "tsx scripts/migrate-emc.ts status",
    "migrate:gamification": "tsx scripts/migrate-gamification.ts",
    "generate:vapid-keys": "tsx scripts/generate-vapid-keys.ts",
    "dr:check": "tsx scripts/clone-and-restore-check.ts run",
    "slo:check": "tsx scripts/slo-checker.ts run",
    "chaos:run": "tsx scripts/chaos-mini.ts run",
    "secrets:rotate": "tsx scripts/rotate-secrets.ts rotate-all",
    "secrets:rotate-single": "tsx scripts/rotate-secrets.ts rotate",
    "ai:diagnose": "tsx ai/self_diagnose.ts",
    "ai:insights": "node ai/insights_agent.mjs",
    "ai:futurecheck": "tsx scripts/futurecheck.ts",
    "ai:embeddings": "node scripts/generate-embeddings.mjs",
    "ai:autoscale": "tsx ai/ai_autoscale.ts",
    "ai:privacy": "tsx ai/privacy_guard.ts",
    "ai:audit": "npm run ai:diagnose && npm run ai:insights && npm run ai:futurecheck && npm run ai:privacy",
    "watchers:db": "tsx watchers/db_integrity.watcher.ts",
    "watchers:api": "tsx watchers/api_contract.watcher.ts",
    "watchers:ai": "tsx watchers/ai_performance.watcher.ts",
    "watchers:all": "npm run watchers:db && npm run watchers:api && npm run watchers:ai",
    "guardian:verify": "tsx ops/cli.ts guardian verify",
    "guardian:audit": "tsx ops/cli.ts guardian audit",
    "guardian:report": "tsx ops/cli.ts guardian report",
    "guardian:status": "tsx ops/cli.ts guardian status",
    "perf:collect": "tsx scripts/collect-metrics.ts",
    "perf:report": "tsx scripts/generate-performance-report.ts",
    "perf:optimize": "tsx scripts/auto-optimize.ts",
    "perf:alert": "tsx scripts/alert-regressions.ts",
    "perf:verify": "tsx scripts/verify-performance-setup.ts",
    "perf:all": "npm run perf:collect && npm run perf:report && npm run perf:optimize && npm run perf:alert"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.8.2",
    "@opentelemetry/api": "^1.8.0",
    "@opentelemetry/auto-instrumentations-node": "^0.47.0",
    "@opentelemetry/exporter-metrics-otlp-http": "^0.47.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.47.0",
    "@opentelemetry/resources": "^1.21.0",
    "@opentelemetry/sdk-metrics": "^1.21.0",
    "@opentelemetry/sdk-node": "^0.47.0",
    "@opentelemetry/semantic-conventions": "^1.21.0",
    "@playwright/test": "^1.40.1",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.16",
    "axe-playwright": "^1.2.0",
    "commander": "^11.1.0",
    "eslint": "^8.55.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "globals": "^13.24.0",
    "husky": "^8.0.3",
    "license-checker": "^25.0.1",
    "lighthouse-ci": "^1.13.1",
    "@lhci/cli": "^0.13.0",
    "pa11y-ci": "^3.1.0",
    "wait-on": "^7.2.0",
    "supabase": "^2.5.0",
    "vercel": "^39.0.0",
    "lint-staged": "^15.2.0",
    "playwright": "^1.40.1",
    "postcss": "^8.4.32",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "prisma": "^5.7.1",
    "rollup-plugin-visualizer": "^5.9.2",
    "stripe": "^14.25.0",
    "tailwindcss": "^3.3.6",
    "terser": "^5.24.0",
    "tsx": "^4.6.2",
    "typescript": "^5.3.0",
    "typescript-eslint": "^7.0.0",
    "vite-plugin-pwa": "^0.17.4",
    "vitest": "^1.0.4",
    "zod": "^3.22.4"
  },
  "engines": {
    "node": ">=18.17.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/your-org"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ],
    "*.{css,scss,sass}": [
      "prettier --write"
    ]
  }
}