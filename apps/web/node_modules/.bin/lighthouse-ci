#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspace/node_modules/.pnpm/lighthouse-ci@1.13.1/node_modules/lighthouse-ci/bin/node_modules:/workspace/node_modules/.pnpm/lighthouse-ci@1.13.1/node_modules/lighthouse-ci/node_modules:/workspace/node_modules/.pnpm/lighthouse-ci@1.13.1/node_modules:/workspace/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspace/node_modules/.pnpm/lighthouse-ci@1.13.1/node_modules/lighthouse-ci/bin/node_modules:/workspace/node_modules/.pnpm/lighthouse-ci@1.13.1/node_modules/lighthouse-ci/node_modules:/workspace/node_modules/.pnpm/lighthouse-ci@1.13.1/node_modules:/workspace/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../lighthouse-ci/bin/cli.js" "$@"
else
  exec node  "$basedir/../lighthouse-ci/bin/cli.js" "$@"
fi
