# ===========================================
# SUPABASE ENTERPRISE SELF-HOSTED CONFIGURATION
# ===========================================

# Database Configuration
POSTGRES_DB=postgres
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_secure_postgres_password_here
POSTGRES_HOST_AUTH_METHOD=scram-sha-256

# Supabase Core Configuration
SUPABASE_PUBLIC_URL=http://localhost:3000
SUPABASE_ANON_KEY=your_anon_key_here
SUPABASE_SERVICE_KEY=your_service_role_key_here

# JWT Configuration
PGRST_JWT_SECRET=your_jwt_secret_here
PGRST_JWT_EXP=3600

# Database Encryption
DB_ENC_KEY=your_database_encryption_key_here
SECRET_KEY_BASE=your_secret_key_base_here

# Logflare Configuration
LOGFLARE_API_KEY=your_logflare_api_key_here
LOGFLARE_LOGGER_BACKEND_API_KEY=your_logflare_backend_api_key_here

# Redis Configuration
REDIS_PASSWORD=your_secure_redis_password_here

# Monitoring Configuration
GRAFANA_ADMIN_USER=admin
GRAFANA_ADMIN_PASSWORD=your_secure_grafana_password_here

# Image Processing
IMGPROXY_ENABLE_WEBP_DETECTION=true

# Functions Configuration
FUNCTIONS_VERIFY_JWT=false

# Organization Configuration
DEFAULT_ORGANIZATION_NAME=AIAS Enterprise
DEFAULT_PROJECT_NAME=AIAS Platform

# ===========================================
# ENTERPRISE SECURITY CONFIGURATION
# ===========================================

# Security Scanning
SECURITY_SCAN_ENABLED=true
SECURITY_SCAN_SCHEDULE=0 2 * * *  # Daily at 2 AM

# Backup Configuration
BACKUP_ENABLED=true
BACKUP_SCHEDULE=0 1 * * *  # Daily at 1 AM
BACKUP_RETENTION_DAYS=30

# Monitoring Alerts
ALERT_EMAIL=admin@yourcompany.com
ALERT_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/SLACK/WEBHOOK

# ===========================================
# COMPLIANCE & PRIVACY CONFIGURATION
# ===========================================

# GDPR/CCPA Compliance
PRIVACY_MODE_ENABLED=true
DATA_RETENTION_DAYS=2555  # 7 years for compliance
CONSENT_MANAGEMENT_ENABLED=true

# Audit Logging
AUDIT_LOG_ENABLED=true
AUDIT_LOG_RETENTION_DAYS=2555

# Data Residency
DATA_RESIDENCY_REGION=US
ENCRYPTION_AT_REST=true
ENCRYPTION_IN_TRANSIT=true

# ===========================================
# PERFORMANCE & SCALING CONFIGURATION
# ===========================================

# Database Performance
POSTGRES_SHARED_BUFFERS=256MB
POSTGRES_EFFECTIVE_CACHE_SIZE=1GB
POSTGRES_WORK_MEM=4MB
POSTGRES_MAINTENANCE_WORK_MEM=64MB

# Redis Performance
REDIS_MAXMEMORY=512mb
REDIS_MAXMEMORY_POLICY=allkeys-lru

# Elasticsearch Performance
ES_HEAP_SIZE=512m
ES_MAX_OPEN_FILES=65536

# ===========================================
# INTEGRATION CONFIGURATION
# ===========================================

# External Services
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
STRIPE_SECRET_KEY=your_stripe_secret_key_here
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret_here

# Email Configuration
SMTP_HOST=smtp.yourcompany.com
SMTP_PORT=587
SMTP_USER=your_smtp_user_here
SMTP_PASSWORD=your_smtp_password_here
SMTP_FROM=noreply@yourcompany.com

# ===========================================
# DEVELOPMENT CONFIGURATION
# ===========================================

# Development Mode
NODE_ENV=development
DEBUG=true
LOG_LEVEL=debug

# Hot Reload
VITE_HMR_PORT=24678
VITE_HMR_HOST=localhost

# ===========================================
# PRODUCTION OVERRIDES
# ===========================================

# Uncomment and configure for production
# NODE_ENV=production
# DEBUG=false
# LOG_LEVEL=info
# SUPABASE_PUBLIC_URL=https://your-domain.com
# POSTGRES_PASSWORD=your_production_postgres_password
# REDIS_PASSWORD=your_production_redis_password
# GRAFANA_ADMIN_PASSWORD=your_production_grafana_password
